<%= render :partial => 'partials/pagination' %>

<% @contacts.each do |contact| %>
    <div class="row contact_box">
      <div class="row  text-center">
        <div class="col-xs-12">
          <%= image_tag contact.spy_image, :class => 'contact_image' %>
        </div>
        <div class="col-xs-12">
          <h1 class="contact_name"><%= contact.name %></h1>
          <h4 class="contact_title"><%= contact.spy_title %></h4>
        </div>

        <div class="col-xs-12">
          <h4 class="contact_email"><%= contact.email %></h4>
        </div>

      </div>
    </div>
<% end %>

<%= render :partial => 'partials/pagination' %>